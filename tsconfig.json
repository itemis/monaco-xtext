  "compilerOptions": {
		"noImplicitAny": false,
		"sourceMap": true,
		"stripInternal": true,

    "module": "amd",
    "moduleResolution": "node",
    "outDir": "./out",
    "target": "es5",
    "lib": [
      "dom",
      "es6"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*"
  ]
}